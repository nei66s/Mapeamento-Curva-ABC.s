{
  "name": "nextn",
  "version": "1.0.0",
  "private": true,
  "engines": {
    "node": "20.x"
  },
  "scripts": {
    "dev": "powershell -NoProfile -ExecutionPolicy Bypass -File scripts/start-dev.ps1",
    "snapshot": "powershell -NoProfile -ExecutionPolicy Bypass -File ./scripts/save-version.ps1",
    "genkit:dev": "genkit start -- tsx src/ai/dev.ts",
    "genkit:watch": "genkit start -- tsx --watch src/ai/dev.ts",
    "build": "cross-env NODE_ENV=production next build",
    "postbuild": "node scripts/fix-missing-manifests.js",
    "prestart": "node scripts/fix-missing-manifests.js",
    "start": "next start",
    "lint": "eslint \"middleware.ts\" \"src\" \"tests\" \"scripts\" --ext .ts,.tsx,.js,.jsx",
    "typecheck": "tsc --noEmit",
    "test": "vitest",
    "test:e2e": "playwright test",
    "test:normalize": "ts-node scripts/test-normalize-status.ts",
    "migrate:store-items": "ts-node src/lib/migrate-store-items.ts",
    "migrate:all": "npx ts-node src/lib/migrate-all-mocks.ts && npx ts-node src/lib/migrate-categories-items.ts && npx ts-node src/lib/migrate-items-schema.ts && npm run migrate:store-items",
    "migrate:indicators": "ts-node src/lib/migrate-indicators.ts",
    "migrate:items-schema": "ts-node src/lib/migrate-items-schema.ts",
    "db:list-unused": "node scripts/list-unused-tables.js",
    "db:drop-unused": "node scripts/drop-unused-by-whitelist.js",
    "db:seed": "node scripts/apply_sql.js --file seed.sql",
    "seed:admin": "node scripts/seed-admin-modules-and-flags.js",
    "db:fix-schema": "node scripts/apply_sql.js --file sql/fix-users-and-unsalvageable.sql",
    "db:fix-tools": "node scripts/apply_sql.js --file sql/fix-tools.sql",
    "db:fix-tools-id": "node scripts/apply_sql.js --file sql/fix-tools-id-to-text.sql",
    "db:fix-stores": "node scripts/apply_sql.js --file sql/fix-stores-add-lat-lng.sql",
    "prod": "powershell -NoProfile -ExecutionPolicy Bypass -File scripts/start-prod.ps1",
    "migrate:account-requests": "powershell -NoProfile -ExecutionPolicy Bypass -File scripts/apply-account-requests-migration.ps1"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.1.0",
    "@dnd-kit/sortable": "^8.0.0",
    "@genkit-ai/google-genai": "^1.15.0",
    "@genkit-ai/next": "1.15.0",
    "@hookform/resolvers": "^3.9.0",
    "@radix-ui/react-accordion": "^1.2.3",
    "@radix-ui/react-alert-dialog": "^1.1.6",
    "@radix-ui/react-avatar": "^1.1.3",
    "@radix-ui/react-checkbox": "^1.1.4",
    "@radix-ui/react-collapsible": "^1.1.11",
    "@radix-ui/react-dialog": "^1.1.6",
    "@radix-ui/react-dropdown-menu": "^2.1.6",
    "@radix-ui/react-label": "^2.1.2",
    "@radix-ui/react-menubar": "^1.1.6",
    "@radix-ui/react-popover": "^1.1.6",
    "@radix-ui/react-progress": "^1.1.2",
    "@radix-ui/react-radio-group": "^1.3.8",
    "@radix-ui/react-scroll-area": "^1.2.3",
    "@radix-ui/react-select": "^2.1.6",
    "@radix-ui/react-separator": "^1.1.2",
    "@radix-ui/react-slider": "^1.2.3",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.1.3",
    "@radix-ui/react-tabs": "^1.1.3",
    "@radix-ui/react-toast": "^1.2.6",
    "@radix-ui/react-tooltip": "^1.1.8",
    "@tanstack/react-query": "^5.62.7",
    "@tanstack/react-query-devtools": "^5.62.7",
    "@vercel/speed-insights": "^1.3.1",
    "bcryptjs": "^2.4.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^3.6.0",
    "dotenv": "^16.6.1",
    "embla-carousel-react": "^8.6.0",
    "firebase": "^11.9.1",
    "genkit": "^1.15.0",
    "html2canvas": "^1.4.1",
    "ioredis": "^5.3.2",
    "js-cookie": "^3.0.5",
    "jspdf": "^3.0.3",
    "leaflet": "^1.9.4",
    "lucide-react": "^0.475.0",
    "next": "^15.5.9",
    "nodemailer": "^6.9.3",
    "patch-package": "^8.0.0",
    "pdfkit": "^0.13.0",
    "pg": "^8.16.3",
    "react": "^18.3.1",
    "react-day-picker": "^8.10.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.54.2",
    "recharts": "^2.15.1",
    "sharp": "^0.34.5",
    "stream-buffers": "^3.0.2",
    "tailwind-merge": "^3.0.1",
    "tailwindcss-animate": "^1.0.7",
    "xlsx": "^0.18.5",
    "zod": "^3.24.2",
    "zustand": "^5.0.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.57.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.0.1",
    "@types/bcryptjs": "^2.4.6",
    "@types/js-cookie": "^3.0.6",
    "@types/leaflet": "^1.9.12",
    "@types/node": "^20.19.24",
    "@types/nodemailer": "^7.0.4",
    "@types/react": "^18.3.27",
    "@types/react-dom": "^18.3.7",
    "cross-env": "^7.0.3",
    "eslint": "^8.57.0",
    "eslint-config-next": "14.2.4",
    "genkit-cli": "^1.15.0",
    "jsdom": "^27.2.0",
    "playwright": "^1.56.1",
    "postcss": "^8",
    "tailwindcss": "^3.4.1",
    "ts-node": "^10.9.2",
    "typescript": "^5",
    "vitest": "^4.0.8"
  }
}
